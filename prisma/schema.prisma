generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Summary {
    id String @id @default(uuid())
    pacerCaseId String
    fullSummary String
    shortSummary String
    sourceDocuments String[]
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}